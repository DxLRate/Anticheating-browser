{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0.1",
  "host_permissions": ["https://*.car-rental-app-c2b4f.web.app/cars*"],
  "permissions": [
    "tabs",
    "storage"
    
    ],
    "background": {
        "service_worker": "background.js"
    },
  "content_scripts": [
    
    {
      "matches": ["https://*.car-rental-app-c2b4f.web.app/*"],
      
      "js": ["/scripts/contentScript.js"]
    },
    {
      "matches": ["https://*.car-rental-app-c2b4f.web.app/cars*"],
      "js": ["/scripts/endTest.js"]
    }
  ]
}